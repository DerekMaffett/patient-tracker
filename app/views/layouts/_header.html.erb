<div class="fullwidth">
  <div class="wrap_section_narrow_top">
    <div class="row">
      <div class="span_s">
        <div class="thumb_small">
          <%= image_tag 'swedish.jpg' %>
        </div>
      </div>
      <div class="header_title">
        <%= yield :header_title %>
        <% if current_user %>
            <p>  Logged in as <%= current_user.email %>.</p>
        <% else %>
            <p> <%= link_to 'Log in or Sign Up', new_user_session_path, data: {transition: 'flip'} %></p>


        <% end %>


      </div>

    </div>
  </div>
</div>
